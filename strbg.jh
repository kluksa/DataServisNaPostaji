entity ReceiptsDetailedPositions{
/** BCP0051 */
  rpoid String  required maxlength(36),
  roid String maxlength(36),
  poid String maxlength(36),
  rpvatcode String maxlength(10),
  rpvat Float ,
  rpquantity Float ,
  rpprice Float ,
  rpdiscount Float ,
  rpdiscoabs Float ,
  rploccod String maxlength(16),
  prlocdesc String maxlength(64),
  rpreflfgid Integer ,
  rprefopnr String maxlength(10),
  rprefproid Integer
}

relationship ManyToOne {
    ProductGroups{productMainGroup} to ProductMainGroup
    ReferenceProducts{productGroup} to ProductGroups
    Products{operation} to Operations
	Receipts{operation} to Operations,
    ReceiptsDetailedPositions{receipts} to Receipts,
	OpenInvoices{operation} to Operations,
    OpenInvoices{customerAddress} to CustomerAddresses,
    OrgStructNoCostCenter{operation} to Operations, 
    OrgStructNoCostCenter{authorization} to Authorization,
    OrgStructWCostCenter{operation} to Operations, 
    OrgStructWCostCenter{authorization} to Authorization,
	ConstructionSite{operation} to Operations,
	Authorization{operation} to Operations,
	CondensedAuthorization{operation} to Operations,
    AddressGroups{operation} to Operations


}


relationship OneToOne {
	Receipts{customerAddress} to CustomerAddresses,
	Receipts{constructionSite} to ConstructionSite,
    ReceiptsDetailedPositions{product} to Products,
	AddressGroups{customerAddress} to CustomerAddresses{addressGroup},
	Operations{customerAddress} to CustomerAddresses{Operation},
    CustomerAddresses{constructionSite} to ConstructionSite{customerAddress},
	Products{referenceProduct} to ReferenceProducts{product}
}




// Set pagination options
//paginate JobHistory, Employee with infinite-scroll
paginate all with pagination

dto * with mapstruct

// Set service options to all except few
service all with serviceImpl
// Set an angular suffix
angularSuffix * with mySuffix
