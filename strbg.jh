amgcode String minlength(2) maxlength(2) ,
amgdesc String minlength(64) maxlength(64) ,
amgrefid BigDecimal
}

entity ProductGroups {
/** BCP0021 */
aagoid String required minlength(36) maxlength(36),
amgoid String minlength(36) maxlength(36),
aagcode String minlength(4) maxlength(4) ,
aagdesc String minlength(64) maxlength(64) ,
aagrefid BigDecimal,
amgrefid BigDecimal
}


